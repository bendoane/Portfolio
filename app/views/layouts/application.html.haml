      .container
        .navbar-header
          .navbar-brand
            .dark-logo{:width => "95"}
              = link_to "Ben Doane", root_url
          %button.navbar-toggle{"data-target" => "#custom-collapse", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
        #custom-collapse.collapse.navbar-collapse
          %ul.nav.navbar-nav.navbar-right
            %li.dropdown
              %a{"data-toggle" => "dropdown", :href => ""} Portfolio
              %ul.dropdown-menu{:role => "menu"}
                %li.dropdown
                  = link_to "All the Swing Dancing", "http://www.alltheswingdancing.com"
                %li.dropdown
                  = link_to "Todoodles", "https://salty-journey-3187.herokuapp.com/"
                %li.dropdown
                  = link_to "Github", "https://github.com/bendoane"
            -# %li.dropdown
            -#   %a{"data-toggle" => "dropdown", :href => ""} Blog
            -#   %ul.dropdown-menu{:role => "menu"}
            -#     %li.dropdown
            -#       = link_to "Building Code", "http://uptobuildingcode.blogspot.com/"
            %li.dropdown
              %a{"data-toggle" => "dropdown", :href => ""} Contact
              %ul.dropdown-menu{:role => "menu"}
                %li.dropdown
                  %a{"data-toggle" => "dropdown", :href => "mailto:ben.doane@gamil.com"} ben.doane@gmail.com
                %li.dropdown
                  %a{"data-toggle" => "dropdown"} Ph. 812-499-1621
                %li.dropdown
                  = link_to "Linked In", "https://www.linkedin.com/in/ben-doane-b9040348"
                %li.dropdown
                  = link_to "Resume", resume_path
